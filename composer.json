{
    "name": "mvdstam/ardent-translatable",
    "description": "Ardent Models with better Translatable support",
    "keywords": ["validation", "laravel", "framework", "eloquent", "database", "sql", "orm", "activerecord", "active record", "ardent", "translatable"],
    "type": "library",
    "homepage": "https://github.com/mvdstam/ardent-translatable/",
    "license": "BSD-3-Clause",
    "authors": [
        {
            "name": "Max van der Stam",
            "role": "Developer"
        }
    ],
    "support": {
        "issues": "https://github.com/mvdstam/ardent-translatable/issues"
    },
    "require": {
        "php": ">=5.4.0",
        "dimsav/laravel-translatable": "~4.5",
        "laravelbook/ardent": "2.*"
    },
    "require-dev": {
        "orchestra/testbench": "~2.2",
        "phpunit/phpunit": "~4.0",
        "mockery/mockery": "~0.9"
    },
    "autoload": {
        "psr-4": {
            "Mvdstam\\ArdentTranslatable\\": "src/"
